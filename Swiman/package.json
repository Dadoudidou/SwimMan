    "moduleDirectories": [ "node_modules", "__scripts__" ],
    "globals": {
      "__TS_CONFIG__": "__scripts__/tsconfig.json"
    }
  },
  "betterScripts": {
    "publish:production": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
